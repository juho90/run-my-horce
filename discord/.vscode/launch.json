{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "디스코드 봇",
      "program": "${workspaceFolder}/main.js",
      "envFile": "${workspaceFolder}/.env",
      "console": "integratedTerminal",
      "runtimeExecutable": "nodemon",
      "runtimeArgs": ["--experimental-modules"],
      "skipFiles": ["<node_internals>/**"],
      "restart": true
    }
  ]
}
